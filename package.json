{
  "name": "collapse-regex-char-set",
  "version": "1.0.0",
  "description": "Condense a set of words into a regular expression pattern",
  "author": "Drew Keller <drew@wimpyprogrammer.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run clean && babel src -d lib --extensions '.js,.ts'",
    "clean": "rimraf lib/*",
    "format": "prettier --write 'src/**'",
    "lint": "tsc; tslint -p ./",
    "precommit": "pretty-quick --staged",
    "test": "jest --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/?*.(js|ts)"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/?*.spec.(js|ts)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
