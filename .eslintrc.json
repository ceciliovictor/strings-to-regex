{
	"parser": "@typescript-eslint/parser",
	"extends": [
		"airbnb-base",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier",
		"prettier/@typescript-eslint"
	],
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"settings": {
		"import/parsers": {
			"@typescript-eslint/parser": [
				".ts"
			]
		},
		"import/resolver": {
			"typescript": {}
		}
	}
}
